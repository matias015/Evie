
var init = time()

fn fibonacci(n){

    if n == 0{
        return 0
	}
    if n == 1{
        return 1
	}
    return fibonacci(n-1) + fibonacci(n-2)
}



// 16 -> aprox 3.667 seg
// 17 -> aprox 10.1 seg
var u = 10
var r = 0
var i = 0
loop{
    i = i+1
    if i >= u{
        break
    }
    
    r = r+ fibonacci(i)
}
print(r)



var end = time()

print("Total time: ", (end-init)/1000, " seg")
